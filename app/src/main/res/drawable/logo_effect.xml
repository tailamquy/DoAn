<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <item android:width="70dp"
        android:height="70dp">
        <shape

            android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="#292626"
                android:endColor="#2B282B">
            </gradient>

        </shape>
    </item>

    <item
        android:left="4dp"
        android:right="4dp"
        android:top="4dp"
        android:bottom="4dp">
        <shape
            android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="#1D1C1D"
                android:endColor="#00575757">
            </gradient>

        </shape>
    </item>
    <item
        android:left="3dp"
        android:right="3dp"
        android:top="3dp"
        android:bottom="3dp">
        <shape
            android:shape="oval">
            <gradient
                android:angle="270"
                android:startColor="#272326"
                android:endColor="#00141314">
            </gradient>

        </shape>
    </item>
    <item android:width="50dp"
        android:height="50dp"
        android:gravity="center"
        >
        <bitmap android:src="@drawable/avone"></bitmap>

    </item>
</layer-list>